{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "..",
    "paths": {
      "@/*": ["../src/*"],
      "fumadocs-mdx:collections/*": ["../.source/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "content/**/*.mdx",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules", "eslint.config.mjs", "components/registry/**/*"]
}
